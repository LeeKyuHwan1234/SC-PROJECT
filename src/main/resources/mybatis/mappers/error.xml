<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.camping.camp.dao.ErrorDao">
		<insert id="insertErrorCorrection" parameterType="HashMap">
     	INSERT INTO error_correction(etitle,econtent, ewriter, ewrite_date) VALUES (#{etitle},#{econtent},#{ewriter},NOW())
     </insert>
     <select id="getErrorCorrection"  resultType="ErrorDto">
     		SELECT * FROM error_correction
     </select>
</mapper>